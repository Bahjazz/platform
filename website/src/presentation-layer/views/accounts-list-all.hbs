<div class="content">
   <h1>All account</h1>
{{#if errors.length}}
	 <p>Oh no, something went wrong:</p>
	  <ul>
			{{#each errors}}
				<li>{{this}}</li>
			{{/each}}
	   </ul>
{{else}}
	{{#if accounts.length}}
       {{#each accounts}}
	<div class="cont">
		 <li>{{accountID}}.  {{username}}</li>
	</div>
	   {{/each}}
{{else}}
		<p>Accounts doesn't exist.</p>
	{{/if}}
{{/if}}
</div>
